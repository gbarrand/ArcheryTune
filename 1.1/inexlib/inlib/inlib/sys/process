#ifndef inlib_sys_process
#define inlib_sys_process

#ifdef WIN32
#include <process.h>
#else
#include <unistd.h>
#endif

namespace inlib {

inline int process_id() {
  return static_cast<int>(::getpid());
}

}

#endif
