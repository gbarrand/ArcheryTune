#ifndef inlib_zfunc
#define inlib_zfunc

#include <ostream>

namespace inlib {

typedef bool (*unzip_func)(std::ostream&,
                           unsigned int,const char*,
                           unsigned int,char*,
                           unsigned int&);

typedef bool (*zip_func)(std::ostream&,
                         unsigned int,
                         unsigned int,const char*,
                         unsigned int,char*,
                         unsigned int&);

}

#endif
